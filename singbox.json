 "dns": 
 {
   "servers": 
   [
     {"tag": "dns_direct", "address": "223.5.5.5", "address_strategy": "ipv4_only", "strategy": "ipv4_only", "detour": "Alldirect"},
     {"tag": "dns_proxy", "address": "tls://8.8.8.8", "address_strategy": "ipv4_only", "strategy": "ipv4_only", "detour": "Final"}
   ],
   "rules": 
   [
     {"outbound": "any", "server": "dns_direct", "disable_cache": true},
     {"clash_mode": "Direct", "server": "dns_direct"},
     {"clash_mode": "Global", "server": "dns_proxy"},
     {"rule_set": "geosite-cn", "action": "route","server": "dns_direct"},
     {"rule_set": "geosite-geolocation-!cn", "server": "dns_proxy"}
   ],
   "final": "dns_proxy",
   "strategy": "ipv4_only"
},

